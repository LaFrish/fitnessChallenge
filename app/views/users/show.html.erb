<button><%=link_to "Edit Profile", edit_user_path(@user, @logs) %></button></br>



<!-- </br><#%= link_to "Log Index", user_logs_path(@user) %></br> -->
<button><%= link_to "See your Logs", user_logs_path(@user, @logs) %></button></br>
    <% @user.logs.each do |log| %>
<% end %>
<h1>Logs</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Water</th>
      <th>Sleep</th>
      <th>Workout</th>
      <th>Food</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.logs.each do |log| %>
      <tr>
        <td><%= log.id %></td>
        <td><%= log.water %></td>
        <td><%= log.sleep %></td>
        <td><%= log.workout %></td>
        <td><%= log.food %></td>
        <td><%= link_to 'Show', user_log_path(@user,log) %></td>
        <td><%= link_to 'Edit', edit_user_log_path(@user,log) %></td>
        <td><%= link_to 'Destroy', [@user,log],  method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<button><%=link_to "Add to Log", new_user_log_path(@user, @logs) %></button></br>

