<h1> <%= @user.firstName %> <%= @user.LastName %> </h1>
<p> Start Weight: <%= @user.startWeight %></p>
<p> Goal Weight:<%= @user.goalWeight %></p>
</br>


<%= button_to "Logs", user_logs_path(@user, @logs) %>
<%= button_to "Goals", user_goals_path(@user, @goals) %>
<%= button_to "Sleeps", user_sleeps_path(@user, @sleeps) %>
<%= button_to "Foods", user_foods_path(@user, @foods) %>
<%= button_to "Waters", user_waters_path(@user, @waters) %>
