<h1> <%= @user.firstName %> <%= @user.LastName %> </h1>
  <p> Start Weight: <%= @user.startWeight %></p>
  <p> Goal Weight:<%= @user.goalWeight %></p>

  <button><%=link_to "Edit Profile", edit_user_path(@user, @logs) %></button></br>
    </br><%= button_to "Logs", user_logs_path(@user, @logs) %>
  <%= button_to "Goals", user_goals_path(@user, @goals) %>
  <%= button_to "Sleeps", user_sleeps_path(@user, @sleeps) %>
  <%= button_to "Foods", user_foods_path(@user, @foods) %>
  <%= button_to "Waters", user_waters_path(@user, @waters) %>
  <%= button_to "Workouts", user_workouts_path(@user, @workouts) %>
  <%= button_to "Logs", user_logs_path(@user, @logs) %>

  </br><%= link_to "Log Index", user_logs_path(@user) %></br>
  <button><%= link_to "See your Logs", user_logs_path(@user, @logs) %></button></br>
      <% @user.logs.each do |log| %>
  <% end %>


<h1>Sleeps</h1>
    <table>
      <thead>
        <tr>
          <th>Sleep id</th>
          <th>Sleep goalMet</th>
          <th>Sleep Point</th>
          <th>Sleep Goal</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @user.sleeps.each do |sleep| %>
          <tr>
            <td></td>
            <td><%= sleep.id %></td>
            <td><%= sleep.goalMet %></td>
            <td><%= sleep.sleepPoint %></td>
            <td><%= sleep.sleepGoal %></td>
            <td><%= link_to 'Show', user_sleep_path(@user, sleep) %></td>
            <td><%= link_to 'Edit', edit_user_sleep_path(@user, sleep) %></td>
            <td><%= link_to 'Destroy',[@user, sleep] , method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <br>
    <button><%=link_to "New Sleep", new_user_sleep_path(@user, @sleeps) %></button></br>
