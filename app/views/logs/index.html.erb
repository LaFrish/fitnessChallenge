      <th>Id</th>
      <th>Water</th>
      <th>Sleep</th>
      <th>Workout</th>
      <th>Food</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.logs.each do |log| %>
      <tr>
        <td><%= log.id %></td>
        <td><%= log.water %></td>
